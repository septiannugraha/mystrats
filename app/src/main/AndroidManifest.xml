<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.go.bpkp.mystrats">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MAPMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.MAPSplashActivity"
            android:label="@string/title_activity_mapsplash"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.MAPLoginActivity"
            android:label="@string/title_activity_maplogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.MAPMenuActivity"
            android:label="@string/title_activity_mapmenu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.MAPDaftarActivity"
            android:label="@string/title_activity_mapdaftar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>